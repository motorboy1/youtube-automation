this.rpc=rpc,this.context=context}}async function executeRPC(rpc,{context,appName,appVersion}){let rpcUrl="";if(rpc.startsWith("rpc://"))rpcUrl=rpc.replace("rpc://","");else return console.error(`[RPC executor] Unsupported RPC format ${rpc}. The rpc was skipped.`),[];const rpcPrefix=`${encodeURIComponent(appName)}/${appVersion}`;rpcUrl.startsWith(rpcPrefix)||(rpcUrl=`${rpcPrefix}/${rpcUrl}`);const extractedURL=extractQueryParameters(rpcUrl),url=new URL(`${imt.apiurl}/rpcs/${extractedURL.url}`,window.location.origin);try{const result=await fetch(url,{body:JSON.stringify({data:{...context,...extractedURL.params}}),method:"POST",headers:{"Content-Type":"application/json"}});if(result.ok)return(await result.json()).response;if(result.status>=400&&result.status<500){const error=await result.json(),subMessages=error.suberrors?.map(err=>({message:err.message,detail:err.detail}))??[];return result.status===424?new RPCError("rpc",subMessages,rpc,context):new RPCError("api",[{detail:error.detail,message:error.message},...subMessages],rpc,context)}else return new RPCError("unknown",[],rpc,context)}catch(ex){throw console.error(`[RPC executor] Unexpected error occurred while processing response for ${rpc} - `,ex),ex}}const executeRPCNested=executeRPC,executeRPCOptions=executeRPC;async function*iterFormanConfig(configs,options={}){yield*iterFormanConfigInternal(configs.map(config2=>({config:config2,nestedOrigin:"none"})),options.rpcContext??null,{context:null,location:[]})}async function*iterFormanConfigInternal(configs,rpcValues,{context,location:location2}){for(const{config:config2,nestedOrigin,parentIndex}of configs){if(config2 instanceof RPCError){yield{status:"error",error:config2,location:location2,nestedOrigin};continue}const name=config2.name??parentIndex?.toString();if(!name){console.error("[ConfigIterator] Config without name is skipped in the iterator");continue}if(yield{status:"success",config:structuredClone(config2),configName:name,location:location2,nestedOrigin},